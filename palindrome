#include <stdio.h>
int main() {
    int num, reversed = 0, remainder, temp;
    scanf("%d", &num);
    temp = num;
    while (num != 0) {
        remainder = num % 10;
        reversed = reversed * 10 + remainder;
        num /= 10;
    }
    if (temp == reversed)
        printf("%d is a palindrome.\n",temp);
    else
        printf("%d is not a palindrome.\n", temp);
    return 0;
}
