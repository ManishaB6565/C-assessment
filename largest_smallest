#include <stdio.h>
int main(){
    int n;
    int largest=0,smallest=9;
    scanf("%d",&n);
    while (n!=0){
        int last=n%10;
        if(last>largest)
            largest=last;
        if(last<smallest)
            smallest=last;
        n/=10;
    }
    printf("%d\n",largest);
    printf("%d",smallest);
    return 0;
}
